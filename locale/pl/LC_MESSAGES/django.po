#: unkot/templates/account/password_reset_from_key.html:8
#: unkot/templates/account/password_reset_from_key_done.html:4
#: unkot/templates/account/password_reset_from_key_done.html:7
msgid "Change Password"
msgstr "Zmień hasło"

#: unkot/templates/account/password_reset.html:7
#: unkot/templates/account/password_reset.html:11
#: unkot/templates/account/password_reset_done.html:6
#: unkot/templates/account/password_reset_done.html:9
msgid "Password Reset"
msgstr "Ustawienie nowego hasła"

#: unkot/templates/account/password_reset.html:16
msgid ""
"Forgotten your password? Enter your e-mail address below, and we'll send you "
"an e-mail allowing you to reset it."
msgstr ""
"Proszę podać adres e-mail. Wyślemy na niego wiadomość umożliwiającą "
"ustawienie nowego hasła."

#: unkot/templates/account/password_reset.html:21
msgid "Reset My Password"
msgstr "Ustaw nowe hasło"

#: unkot/templates/account/password_reset.html:24
msgid "Please contact us if you have any trouble resetting your password."
msgstr "Prosimy o kontakt z nami w razie problemów z ustawieniem nowego hasła."

#: unkot/templates/account/password_reset_done.html:15
msgid ""
"We have sent you an e-mail. Please contact us if you do not receive it "
"within a few minutes."
msgstr ""
"Wysłaliśmy Państwu wiadomość e-mail. Proismy o kontakt, jeśli w ciągu kilku "
"minut nie dotrze ona do Państwa."

#: unkot/templates/account/password_reset_from_key.html:8
msgid "Bad Token"
msgstr "Token nieprawidłowy"

#: unkot/templates/account/password_reset_from_key.html:12
#, python-format
msgid ""
"The password reset link was invalid, possibly because it has already been "
"used.  Please request a <a href=\"%(passwd_reset_url)s\">new password reset</"
"a>."
msgstr ""
"Nieprawidłowe łącze do ustawienia nowego hasła, być może było już użyte. "
"Proszę o <a href=\"%(passwd_reset_url)s\">ustawienie nowego hasła</a>."

#: unkot/templates/account/password_reset_from_key.html:18
msgid "change password"
msgstr "zmień hasło"

#: unkot/templates/account/password_reset_from_key.html:21
#: unkot/templates/account/password_reset_from_key_done.html:8
msgid "Your password is now changed."
msgstr "Hasło zostało zmienione."

